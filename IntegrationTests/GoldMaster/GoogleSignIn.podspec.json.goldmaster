load('//:pod_support_buildable/extensions.bzl', 'pch_with_name_hint')
load('//:pod_support_buildable/extensions.bzl', 'acknowledged_target')
# Add a config setting release for compilation mode
# Assume that people are using `opt` for release mode
# see the bazel user manual for more information
# https://bazel.build/versions/master/docs/bazel-user-manual.html
native.config_setting(
  name = "release",
  values = {
    "compilation_mode": "opt"
  }
  )
objc_library(
  name = "GoogleSignIn",
  enable_modules = 1,
  hdrs = glob(
    [
      "pod_support/Headers/Public/**/*.h"
    ],
    exclude_directories = 1
    ),
  pch = pch_with_name_hint(
    "GoogleSignIn",
    [

    ]
    ),
  includes = [
    "pod_support/Headers/Public/",
    "pod_support/Headers/Public/GoogleSignIn/"
  ],
  sdk_frameworks = [
    "CoreText",
    "SafariServices",
    "Security"
  ],
  deps = [
    ":GoogleSignIn_Bundle_GoogleSignIn",
    ":GoogleSignIn_VendoredFrameworks",
    "@GoogleToolboxForMac//:GoogleToolboxForMac_NSString_URLArguments",
    "@GTMOAuth2//:GTMOAuth2",
    "@GoogleToolboxForMac//:GoogleToolboxForMac_NSDictionary_URLArguments",
    "@GTMSessionFetcher//:GTMSessionFetcher_Core"
  ],
  copts = [

  ] + select(
    {
      "//conditions:default": [
        "-DPOD_CONFIGURATION_RELEASE=0"
      ],
      ":release": [
        "-DPOD_CONFIGURATION_RELEASE=1",
        "-DNS_BLOCK_ASSERTIONS=1"
      ]
    }
    ),
  bundles = [
    ":GoogleSignIn_Bundle_GoogleSignIn"
  ],
  visibility = [
    "//visibility:public"
  ]
  )
acknowledged_target(
  name = "GoogleSignIn_acknowledgement",
  deps = [
    ":GoogleSignIn_VendoredFrameworks_acknowledgement",
    "@GoogleToolboxForMac//:GoogleToolboxForMac_NSDictionary_URLArguments_acknowledgement",
    ":GoogleSignIn_Bundle_GoogleSignIn_acknowledgement",
    "@GTMSessionFetcher//:GTMSessionFetcher_Core_acknowledgement",
    "@GoogleToolboxForMac//:GoogleToolboxForMac_NSString_URLArguments_acknowledgement",
    "@GTMOAuth2//:GTMOAuth2_acknowledgement"
  ]
  )
objc_bundle(
  name = "GoogleSignIn_Bundle_GoogleSignIn",
  bundle_imports = glob(
    [
      "Resources/GoogleSignIn.bundle/**"
    ],
    exclude_directories = 1
    )
  )
acknowledged_target(
  name = "GoogleSignIn_Bundle_GoogleSignIn_acknowledgement",
  deps = [

  ]
  )
objc_framework(
  name = "GoogleSignIn_VendoredFrameworks",
  framework_imports = glob(
    [
      "Frameworks/GoogleSignIn.framework/**"
    ],
    exclude_directories = 1
    ),
  is_dynamic = 0,
  visibility = [
    "//visibility:public"
  ]
  )
acknowledged_target(
  name = "GoogleSignIn_VendoredFrameworks_acknowledgement",
  deps = [

  ]
  )
