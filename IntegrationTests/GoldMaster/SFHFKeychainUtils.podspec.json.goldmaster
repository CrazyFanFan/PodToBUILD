load('//:build_extensions.bzl', 'pch_with_name_hint')
SFHFKeychainUtils_source_headers = glob(
  [
    "security/**/*.h"
  ],
  exclude_directories = 1
  )
SFHFKeychainUtils_extra_headers = glob(
  [
    "bazel_support/Headers/Public/**/*.h"
  ],
  exclude_directories = 1
  )
SFHFKeychainUtils_headers = SFHFKeychainUtils_source_headers + SFHFKeychainUtils_extra_headers
objc_library(
  name = "SFHFKeychainUtils",
  srcs = glob(
    [
      "security/**/*.c",
      "security/**/*.cpp",
      "security/**/*.m",
      "security/**/*.mm"
    ],
    exclude_directories = 1
    ),
  hdrs = SFHFKeychainUtils_headers,
  pch = pch_with_name_hint(
    "SFHFKeychainUtils"
    ),
  includes = [
    "bazel_support/Headers/Public/",
    "bazel_support/Headers/Public/SFHFKeychainUtils/"
  ],
  sdk_frameworks = [
    "Security"
  ],
  visibility = [
    "//visibility:public"
  ]
  )
